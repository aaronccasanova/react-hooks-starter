{
  "name": "react-hooks-starter",
  "version": "0.0.0-semantically-released",
  "description": "Template for packaging react hooks",
  "author": "Aaron Casanova <aaronccasanova@gmail.com> (http://aaroncasanova.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aaronccasanova/react-hooks-starter.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "format": "prettier --write \"**/*.+(js|md|ts|css|sass|less|graphql|yml|yaml|scss|json|vue)\"",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "prebuild": "rimraf dist",
    "start": "webpack --watch --mode development",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": "16.8.4"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "eslint-config-aaronccasanova": "1.1.3",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-html": "5.0.3",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react-hooks": "1.5.1",
    "eslint-plugin-react": "7.12.4",
    "eslint": "5.15.3",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier-config-aaronccasanova": "1.0.0",
    "prettier": "1.16.4",
    "react": "16.8.4",
    "rimraf": "2.6.3",
    "webpack-cli": "3.3.0",
    "webpack": "4.29.6",
    "semantic-release": "^15.13.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
